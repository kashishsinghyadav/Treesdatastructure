class Solution:
    def mergeTwoLists(self, list1: Optional[ListNode], list2: Optional[ListNode]) -> Optional[ListNode]:
        new_list=headnode=ListNode()
        while list1 and list2:
            if list1.val < list2.val:
                headnode.next=list1
                list1=list1.next
            else:
                headnode.next=list2
                list2=list2.next
            headnode=headnode.next

        if list1:
            headnode.next = list1
        if list2:
            headnode.next = list2
        return new_list.next
        
        
